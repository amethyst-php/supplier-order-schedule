{
    "name": "amethyst/supplier-order-schedule",
    "type": "library",
    "keywords": ["laravel", "amethyst", "supplier-order-schedule"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "amethyst/common": "0.1.*",
        "amethyst/supplier": "0.1.*",
        "mtdowling/cron-expression": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*",
        "orchestra/testbench": "3.*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "supplier-order-schedule",
            "namespace": "Amethyst"
        },
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\SupplierOrderScheduleServiceProvider"
            ]
        }
    }
}
